<!DOCTYPE html>
<html>
<head>
  <title>TestingHelp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'layouts/header' %>



	<div class="site_maintenance">This site is under construction. Inconvenience regretted.</div>
	<% flash.each do |key, value| %>
	<div class="flash_<%=key %>">
		<%= value %>
	</div>
	<% end %>

	

<div class="container"> 
		<%= yield %>
</div>

</body>
</html>
